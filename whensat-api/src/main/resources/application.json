{
  "application": {
    "name": "projeto-py"
  },
  "cloud": {
    "config": {
      "uri": "http://127.0.0.1:8000"
    }
  },
  "hal": {
    "config": {
      "migration": {
        "basePath": "../resources/migration/configuration/master.json"
      }
    }
  },
  "logging_config": {
    "version": 1,
    "formatters": {
      "standard": {
        "format": "%(asctime)s %(levelname)s [%(name)s] %(message)s",
        "datefmt": "[%Y-%m-%d %H:%M:%S]"
      }
    },
    "handlers": {
      "default": {
        "level": 20,
        "formatter": "standard",
        "class": "logging.StreamHandler",
        "stream": "ext://sys.stdout"
      },
      "file": {
        "level": 20,
        "formatter": "standard",
        "class": "logging.FileHandler",
        "mode": "a",
        "filename": "../migrations.log"
      }
    },
    "loggers": {
      "": {
        "handlers": [
          "default",
          "file"
        ],
        "level": 20,
        "propagate": true
      }
    }
  }
}
