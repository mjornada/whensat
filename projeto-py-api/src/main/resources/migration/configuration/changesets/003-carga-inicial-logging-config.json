[
  {
    "key": "logging_config",
    "value": {
      "version": 1,
      "formatters": {
        "standard": {
          "format": "%(asctime)s %(levelname)s [%(name)s] %(message)s",
          "datefmt": "[%Y-%m-%d %H:%M:%S]"
        }
      },
      "handlers": {
        "default": {
          "level": "INFO",
          "formatter": "standard",
          "class": "logging.StreamHandler",
          "stream": "ext://sys.stdout"
        },
        "file": {
          "level": "INFO",
          "formatter": "standard",
          "class": "logging.FileHandler",
          "mode": "a",
          "filename": "../../migrations.log"
        }
      },
      "loggers": {
        "root": {
          "handlers": [
            "default",
            "file"
          ],
          "level": "INFO",
          "propagate": true
        },
        "fastapi": {
          "handlers": [
            "default",
            "file"
          ],
          "level": "INFO",
          "propagate": true
        },
        "pyliquibase": {
          "handlers": [
            "default",
            "file"
          ],
          "level": "INFO",
          "propagate": true
        },
        "uvicorn": {
          "handlers": [
            "default",
            "file"
          ],
          "level": "INFO",
          "propagate": true
        }
      }
    }
  }
]